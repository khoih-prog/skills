{
  "name": "hashbox-plugin",
  "version": "0.1.0",
  "description": "OpenClaw plugin that connects an AI agent to the HashBox iOS app via Firebase webhook for push notifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "hashbox",
    "push-notifications",
    "firebase"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "SKILL.md"
  ],
  "openclaw": {
    "extensions": "./dist/index.js",
    "slots": [
      "tool"
    ],
    "catalog": {
      "name": "hashbox-plugin",
      "description": "Connects an AI agent to the HashBox iOS app via Firebase webhook for push notifications"
    }
  }
}
